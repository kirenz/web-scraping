document.write('<link rel="stylesheet" type="text/css" href="//static.advance.net/static/classifieds/realestate/css/showcase-rectangle.css" />');
document.write('<script type="text/javascript">$(document).ready(function() {'); 
document.write('$(".showcase-ad-rectangle").click(function() {if ($(this).data("source_id")) {');
document.write('processCommLink($(this).data("source_id"),$(this).data("customer_id"),$(this).data("ad_id"),$(this).data("zip"));} else {');
document.write('processLink($(this).data("ad_id"),$(this).data("category_id"),$(this).data("customer_id"),$(this).data("broker_id"),$(this).data("zip"));}');
document.write('window.open($(this).data("href"), "_blank");return false;});');
document.write('function dynamicReportingCrumb(adcat_id,cust_id,broker_id,ad_id,pc) {');
document.write('var TRsrc = "//report-re.cleveland.com/images/mdwtc/MDRE_SITE/" + adcat_id + ".gif?aid="+cust_id+";bid="+broker_id+";lid="+ad_id+";pc="+pc;');
document.write('if (adcat_id) insertTrackingElement(TRsrc);}');
document.write('function dynamicApartmentCrumb(source_id,mgt_co_id,com_id,pc) {');
document.write('var trackingURL = "//report-re.cleveland.com/images/mdwtc/MDAPT_SITE/" + source_id + ".gif?com_id=" + com_id + ";mgmt_co_id=" + mgt_co_id + ";pc=" + pc;');
document.write('if (source_id) insertTrackingElement(trackingURL);}');
document.write('function insertTrackingElement (url) {');
document.write('var element;');
document.write('element = document.createElement("img");');
document.write('element.setAttribute("src", url);');
document.write('element.setAttribute("border", "0");');
document.write('element.setAttribute("height", "1");');
document.write('element.setAttribute("width", "1");');
document.write('document.body.insertBefore(element,null);');
document.write('}function processLink (ad_id,category_id,customer_id,broker_id,ad_zip) {dynamicReportingCrumb(category_id,customer_id,broker_id,ad_id,"483");}');
document.write('function processCommLink (source_id,mgt_co_id,com_id,ad_zip) {dynamicApartmentCrumb(source_id,mgt_co_id,com_id,"485");}');
document.write('});');
document.write('</sc' + 'ript>');

